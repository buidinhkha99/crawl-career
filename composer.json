{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-imagick": "*",
        "adrianorosa/laravel-geolocation": "^1.1",
        "artesaos/seotools": "^1.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-snappy": "^1.0",
        "bensampo/laravel-enum": "^6.2",
        "bepsvpt/secure-headers": "^7.4",
        "guzzlehttp/guzzle": "^7.2",
        "harishdurga/laravel-quiz": "^1.2",
        "html2text/html2text": "^4.3",
        "inertiajs/inertia-laravel": "^0.6.9",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^9.19",
        "laravel/octane": "^1.4",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "repat/laravel-validator-emojis": "^0.2.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3",
        "spatie/laravel-permission": "^6.0",
        "spatie/period": "^2.4",
        "tanmuhittin/laravel-google-translate": "^2.2",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": []
}
