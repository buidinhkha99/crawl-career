{
  "name": "salt/reset-password",
  "description": "A Laravel Nova tool.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Salt\\ResetPassword\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Salt\\ResetPassword\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
